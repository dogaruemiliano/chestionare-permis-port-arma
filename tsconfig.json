{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "target": "es6",
    "module": "esnext",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@constants/*": ["./src/constants/*"],
      "@models/*": ["./src/models/*"],
      "@navi  gation/*": ["./src/navigation/*"],
      "@screens/*": ["./src/screen/*"],
      "@services/*": ["./src/services/*"],
      "@store/*": ["./src/store/*"],
      "@store": ["./src/store/index.ts"]
    },
    "noUnusedLocals": false
  }
}
